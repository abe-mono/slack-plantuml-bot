{
  "name": "slack-plantuml-bot",
  "version": "1.0.0",
  "description": "",
  "main": "bundle.js",
  "engins": {
    "node": ">=9.2.0"
  },
  "scripts": {
    "build": "webpack",
    "watch": "webpack -w",
    "test": "jest --no-cache",
    "docker-build": "cp ./build/bundle.js ./dockernize/slack-plantuml-bot.js && docker build -t $npm_package_name:$npm_package_version ./dockernize",
    "test-dump": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazuhito-m/slack-plantuml-bot.git"
  },
  "author": "@kazuhito_m",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kazuhito-m/slack-plantuml-bot/issues"
  },
  "homepage": "https://github.com/kazuhito-m/slack-plantuml-bot#readme",
  "jest": {
    "testRegex": ".*/test/.*Test\\.ts$",
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "awesome-typescript-loader": "^3.4.0",
    "jest": "^21.2.1",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^21.2.4",
    "typescript": "^2.6.2",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "botkit": "^0.6.7"
  }
}
